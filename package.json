{
  "name": "@dingshaohua.com/hybrid-crypto",
  "version": "0.0.1-alpha.3",
  "homepage": "https://github.com/dingshaohua-com/hybrid-crypto",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dingshaohua-com/hybrid-crypto.git"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/server/index.js",
  "types": "types/server/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/server/index.d.ts",
      "default": "./dist/server/index.js"
    },
    "./browser": {
      "types": "./types/browser/index.d.ts",
      "default": "./dist/browser/index.js"
    },
    "./server": {
      "types": "./types/server/index.d.ts",
      "default": "./dist/server/index.js"
    }
  },
  "scripts": {
    "build": "npm run build:browser && npm run build:server",
    "build:browser": "tsc -p tsconfig.browser.json",
    "build:server": "tsc -p tsconfig.server.json"
  },
  "files": [
    "dist",
    "types"
  ],
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "typescript": "^5.9.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.12.1",
  "dependencies": {
    "@types/node": "^24.3.0",
    "crypto": "^1.0.1",
    "fs-extra": "^11.3.1"
  }
}
